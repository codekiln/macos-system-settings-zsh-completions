#compdef settings

_macos_system_settings() {
    # We'll call _get_system_settings (from plugin) to get lines
    local -a panels
    panels=($(_get_system_settings))

    # Possibly prettify each line if you like. 
    # For now, just pass them directly:
    _describe -V 'settings panel' panels
}

_macos_system_settings "$@"